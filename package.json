{
  "name": "ngx-keys",
  "version": "1.2.0",
  "description": "A reactive Angular library for managing keyboard shortcuts with signals-based UI integration",
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrivasperez/ngx-keys.git"
  },
  "bugs": {
    "url": "https://github.com/mrivasperez/ngx-keys/issues"
  },
  "homepage": "https://github.com/mrivasperez/ngx-keys#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test ngx-keys --browsers=ChromeHeadless",
    "test:ci": "ng test ngx-keys --no-watch --no-progress --browsers=ChromeHeadless"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
